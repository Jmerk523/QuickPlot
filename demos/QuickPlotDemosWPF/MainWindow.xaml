<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuickPlotDemosWPF"
        xmlns:WPF="clr-namespace:QuickPlot.WPF;assembly=QuickPlot.WPF" x:Class="QuickPlotDemosWPF.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <StackPanel Grid.Column="0" Grid.Row="0" Orientation="Horizontal">
            <Button Margin="3" Content="Render" Name="btnRender" Click="btnRender_Click"/>
            <Button Margin="3" Content="Benchmark" Name="btnBenchmark" Click="btnBenchmark_Click"/>
        </StackPanel>
        <WPF:InteractivePlot Grid.Column="0" Grid.Row="1" Name="interactivePlot1" />
    </Grid>
</Window>
